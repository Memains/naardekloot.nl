{
	"manifest_version": 2,
	"name": "Cloud To Kloot",
	"version": "1.0",
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
	"description": "Replaces the text 'cloud' with 'kloot'",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

